<input:>
    <input id="{{=getVar('fieldId')}}" name="{{identifier}}" disabled="{{disabled}}" tabindex="{{tabIndex}}" as="fieldElement"
        class="b-input-field-h" readonly="{{readOnly}}" maxlength="{{maxLength}}"
        type="{{if password && masked}}password{{else}}text{{/if}}" value="{{instantValue}}">
</input:>