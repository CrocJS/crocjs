<cls:><super/> type_text</cls:>

<input:>
    <input id="{{=getVar('fieldId')}}" name="{{identifier}}" readonly="{{readOnly}}" maxlength="{{maxLength}}"
        class="b-input-field-h" disabled="{{disabled}}" tabindex="{{tabIndex}}" as="fieldElement"
        type="{{if password && masked}}password{{else}}text{{/if}}" value="{{instantValue}}">
</input:>