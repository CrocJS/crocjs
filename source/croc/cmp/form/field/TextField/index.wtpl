<cls:>{{super cls}} type_text</cls:>

<input:>
    <input type="{{if password && masked}}password{{else}}text{{/if}}" id="{{=getVar('fieldId')}}" name="{{identifier}}" value="{{instantValue}}"
        class="b-input-field-h" disabled="{{disabled}}" tabindex="{{tabIndex}}" as="fieldElement"
        maxlength="{{maxLength}}" readonly="{{readOnly}}">
</input:>